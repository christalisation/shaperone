{
  "name": "@hydrofoil/shaperone-wc-shoelace",
  "version": "0.3.2",
  "type": "module",
  "scripts": {
    "babel": "babel . --out-dir . --extensions .ts --config-file ../../babel.config.json",
    "analyze": "run-p analyze:*",
    "analyze:wc": "wca --format json --outFile ./custom-elements.json",
    "analyze:readme": "wca --outFile ./README.md",
    "analyze:lit": "lit-analyzer . --strict --quiet --rules.no-invalid-css off"
  },
  "exports": {
    "./components.js": "./components.js",
    "./component-extras.js": "./component-extras.js",
    "./templates.js": "./templates.js"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "custom-elements.json"
  ],
  "dependencies": {
    "@hydrofoil/shaperone-core": "^0.11.0",
    "@hydrofoil/shaperone-wc": "^0.7.13",
    "@rdfine/shacl": "^0.10.5",
    "@rdfjs-elements/lit-helpers": "^0.3.7",
    "@shoelace-style/shoelace": ">=2.0.0-beta.77 < 3",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "is-graph-pointer": "^2.1.0",
    "lit": "^2",
    "p-debounce": "^4"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.6",
    "@zazuko/env": "^2.0.6",
    "@shaperone/testing": "*"
  },
  "repository": "https://github.com/hypermedia-app/shaperone",
  "author": "Tomasz Pluskiewicz <awesome@hypermedia.app>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
